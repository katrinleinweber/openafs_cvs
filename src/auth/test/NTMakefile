	$(DESTDIR)\lib\afs\afsutil.lib \
	$(DESTDIR)\lib\afs\afsauth.lib \
	$(DESTDIR)\lib\afs\afsreg.lib \
	$(DESTDIR)\lib\afs\afspioctl.lib

$(KTC_EXEFILE): $(KTC_EXEOBJS)  $(KTC_EXELIBS)
	$(EXECONLINK)
	$(EXEPREP) 

test tests: $(CELL_EXEFILE) $(KTC_EXEFILE)

