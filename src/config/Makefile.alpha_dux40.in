AFS_OSTYPE = DUX

MT_CC=cc
XCFLAGS= -D_NO_PROTO -DOSF
MT_CFLAGS=-D_REENTRANT=1 -pthread -DAFS_PTHREAD_ENV ${XCFLAGS}
SHLIB_SUFFIX=so
SHLIB_CFLAGS=
XLIBS=@LIB_AFSDB@
MTLIBS=-pthread -lpthread -lmach -lexc -lc
NON_SHARED=-non_shared
