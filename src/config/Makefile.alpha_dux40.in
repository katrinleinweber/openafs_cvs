OPTMZ=-O2
DBG=-g3
CSTATIC=-non_shared
RANLIB=/bin/true
INSTALL=${TOP_SRCDIR}/pinstall/pinstall
CP=cp

#this is for the vol package
FS_CONV_OSF40D         = fs_conv_dux40D
install_FS_CONV_OSF40D = $(DESTDIR)${afssrvsbindir}/fs_conv_dux40D
