# nmake Makefile for NT build of LWP. 

!include ..\config\NTMakefile.$(SYS_NAME)
!include ..\config\NTMakefile.version

LIBOBJS = $(OJT)\lock.obj \
	  $(OJT)\lwp_nt.obj \
	  $(OJT)\iomgr.obj \
	  $(OJT)\timer.obj\
	  $(OJT)\fasttime.obj \
	  $(OJT)\waitkey.obj \
	  $(OJT)\threadname.obj \
	  $(OJT)\AFS_component_version_number.obj

LIBFILE = $(DESTDIR)\lib\afslwp.lib

INCFILES = \
	$(DESTDIR)\include\lock.h \
	$(DESTDIR)\include\lwp.h \
	$(DESTDIR)\include\preempt.h \
	$(DESTDIR)\include\timer.h



install: $(LIBFILE) $(INCFILES)

install9x: install

$(LIBFILE): $(LIBOBJS)
	$(LIBARCH)

tests: $(OJT)\rw.exe

$(OJT)\rw.exe: $(OJT)\rw.obj $(LIBFILE) $(DESTDIR)\lib\afs\afsutil.lib
	$(EXECONLINK)

clean::
	$(DEL) $(LIBFILE)

mkdir:
	
