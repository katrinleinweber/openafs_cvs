sub debug {
&Print("Installs the following into /usr/vice/bin:");
&Print("  bin/afsmonitor");
&Print("  bin/cmdebug");
&Print("  etc/fstrace");
&Print("  etc/kdump");
&Print("  etc/rxdebug");
&Print("  bin/scout");
&Print("  bin/udebug");
&Print("Install root.server/usr/afs/bin/volinfo into /usr/afs/bin");
&Print("Install root.client/usr/vice/etc/C/afszcm.cat into /usr/vice/etc/C");
}
